          "media": {
            "image": {
              "active": true,
              "width": 1400,
              "src": "@PROJECT_PATH@/assets/images/kemble-wetland-nature-reserve-map-image-620x478.jpg",
              "link": "assets/files/Kemble_Wetland_Nature_Reserve_Map.pdf",
              "alt": "Kemble Wetland Nature Reserve Map",
              "title": "Kemble Wetland Nature Reserve Map",
              "linkInNewWindow": true
            },
            "video": {
              "src": "https://www.youtube.com/embed/Tq7p44IA-1U",
              "aspectratio": "16:9",
              "autoplay": false,
              "loop": false
